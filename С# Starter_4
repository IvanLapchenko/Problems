using System;


class Program
{
    public static void Main(string[] args)
    {
        repeater("repeater", 5);
    }

    public static void repeater(string stringToRepeat, int numberOfRepetitions)
    {
        for (int i = 0; i < numberOfRepetitions; i++)
        {
            Console.WriteLine(stringToRepeat);
        }
    }
}
